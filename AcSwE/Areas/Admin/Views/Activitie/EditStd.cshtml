
@{
    ViewBag.Title = "EditStd";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h3>EditStd</h3>

<table class="table">
    <tr>
        <th>
          <h3>Id STD</h3>
        </th>

        <th>
            <h3>Full Name</h3>
        </th>

        <th>
            <h3>Year</h3>
        </th>
        <th></th>
    </tr>

    @foreach (var a in ViewBag.std)
    {
        <tr>
            <td>
                <h5>@a.idStd</h5>
            </td>

            <td>
                <h5>@a.title@a.firstName @a.lastName</h5>
            </td>

            <td>
                <h5>@a.year</h5>
            </td>        

            <td>
                @Html.ActionLink("Edit", "EditStds", new { id = @a.id , Acid = ViewBag.acid }) |
                @Html.ActionLink("Details", "DetailsStd", new { id = @a.id, Acid = ViewBag.acid }) |
                @Html.ActionLink("Delete", "DeleteStd", new { id = @a.id, Acid = ViewBag.acid, idstd = @a.idStd})
            </td>
        </tr>
    }
</table>

<div>
    @Html.ActionLink("Save Change", "Index") | 
    @*@Html.ActionLink("Add Student with import .xlxs", "Index")*@
</div>